        <Label x:Name="lblTitle" Content="lblTitle" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="22" FontWeight="Bold" Padding="0" Background="{x:Null}" Foreground="#FF555555"/>
